{
  "filesMatching": [
    {
      "glob": "**/*.html",
      "actions": [
        {
          "type": "fixLinks"
        },
        {
          "type": "removeElement",
          "selector": ".w-dyn-empty"
        }
      ]
    },
    {
      "glob": "detail_post.html",
      "actions": [
        {
          "selector": ".section-14 .w-container *",
          "type": "includePartial",
          "partial": "_post.html",
          "treatAsOneMatch": true
        }
      ]
    },
    {
      "glob": "about-us.html",
      "actions": [
        {
          "selector": ".collection-item-4",
          "type": "includePartial",
          "partial": "_team_members.html"
        }
      ]
    },
    {
      "glob": "career.html",
      "actions": [
        {
          "selector": ".collection-list-wrapper-3 .w-dyn-item",
          "type": "includePartial",
          "partial": "_jobs_open_top.html"
        },
        {
          "selector": ".collection-list-wrapper-2 .w-dyn-item",
          "type": "includePartial",
          "partial": "_jobs_open_all.html"
        }
      ]
    },
    {
      "glob": "detail_projects.html",
      "actions": [
        {
          "selector": "div[class=w-container]",
          "type": "includePartial",
          "partial": "_project.html",
          "treatAsOneMatch": true
        }
      ]
    },
    {
      "glob": "projects.html",
      "actions": [
        {
          "selector": ".w-dyn-items .w-dyn-item",
          "type": "includePartial",
          "partial": "_projects_list.html"
        }
      ]
    },
    {
      "glob": "**/*.html",
      "actions": [
        {
          "selector": ".footer_section, .footer_section ~ *",
          "type": "includePartial",
          "partial": "_footer.html",
          "treatAsOneMatch": true
        },
        {
          "selector": "[data-w-id=c580002b-9f94-1c00-b7e4-e5af8321f4c1]",
          "type": "includePartial",
          "partial": "_top_navigation.html"
        },
        {
          "selector": "body",
          "type": "wrapLayout",
          "layout": "default"
        }
      ]
    }
  ]
}
